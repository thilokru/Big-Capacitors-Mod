{
  "parameters": {
    "world_to_cycle": [ "/", "#cycle_length" ],
    "round_cycle": [ "compose", [ "R", "#cycle_length" ] , "#click_time" ],
    "end_cycle": [ "-", "#round_cycle" ]
  },
  "clips": {
    "default": "big_capacitors:/block/blockCrusher@default",
    "starting": [ "trigger_positive", "#default", "#end_cycle", "!transition:moving" ],
    "moving": [ "apply", "big_capacitors:/block/blockCrusher@moving", "#world_to_cycle" ],
    "stopping": [ "trigger_positive", "#moving", "#end_cycle", "!transition:default" ]
  },
  "states": [
    "default",
    "starting",
    "moving",
    "stopping"
  ],
  "transitions": {
    "default": "starting",
    "starting": "moving",
    "moving": "stopping",
    "stopping": "default"
  },		
  "start_state": "default"
}